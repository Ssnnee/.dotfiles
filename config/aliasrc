#aliases
alias vim=nvim
alias d='cd ~/.coding/'
alias m='cd ~/Music'
alias t='cd ~/Downloads'
alias h='cd ~/'
alias i='cd ~/.config/i3'
alias ..='cd ..'
alias lf='/home/sne/go/bin/lf'
alias v="vim ~/.config/nvim/"
alias c="cd ~/.config"
alias cmatrix="cmatrix -C white"
alias e="cd ~/.ecole/"

ff() {cd "$(find ~/.coding -maxdepth 1 -type d | fzf)" ; $EDITOR .}
fe() {cd "$(find ~/.ecole -maxdepth 1 -type d | fzf)" ; $EDITOR .}
