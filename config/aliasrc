#aliases
alias vim=nvim
alias d='cd ~/.coding/'
alias m='cd ~/Music'
alias t='cd ~/Downloads'
alias h='cd ~/'
alias i='cd ~/.config/i3'
alias ..='cd ..'
alias lf='/home/sne/go/bin/lf'
alias v="vim ~/.config/nvim/"
alias c="cd ~/.config"
alias cmatrix="cmatrix -C white"

fd() { cd "$(du -sh ~/.coding/* ~/.config/* | awk '{ print $2 }' | fzf)" && $EDITOR . }
